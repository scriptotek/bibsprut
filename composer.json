{
    "name": "scriptotek/blekkio",
    "description": "Blekkio",
    "keywords": ["video", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.0.0",
        "laravel/framework": "5.5.*",
        "alaouy/youtube": "~1.2",
        "graham-campbell/markdown": "~8.0",
        "cebe/markdown": "~1.1",
        "sabre/dav": "~3.0",
        "pulkitjalan/google-apiclient": "~3.1",
        "zendframework/zend-feed": "^2.5",
        "zendframework/zend-servicemanager": "^2.6",
        "ramsey/uuid": "~3.0",
        "php-ffmpeg/php-ffmpeg": "0.6.*",
        "league/flysystem-webdav": "~1.0",
        "laravelcollective/html": "5.5.*",
        "fabpot/goutte": "^3.2",
        "laravel/tinker": "^1.0",
        "twig/twig": "^2.3",
        "aacotroneo/laravel-saml2": "^0.8.1",
        "sentry/sentry": "^1.6",
        "sentry/sentry-laravel": "^0.7.0",
        "pubsubhubbub/subscriber": "^1.1",
        "danmichaelo/quitesimplexmlelement": "~1.0",
        "icyapril/tail": "^1.0"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~6.0",
        "phpspec/phpspec": "~2.1",
        "symfony/dom-crawler": "~3.0",
        "symfony/css-selector": "~3.0"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
